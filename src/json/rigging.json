            },
            {
                "name": "iks",
                "class": "Array",
                "extends": "IK"
            },
            {
                "name": "controllers",
                "class": "Array",
                "extends": "Controller"
            },
            {
                "names": "constraints",
                "class": "Array",
                "extends": "Constraint"
            }
        ]
    },
    "RigBase": {
        "has_a": ["Digest", "Position"]
    },
    "Node": {
        "extends": "RigBase"
    },
    "IK": {
        "attributes": [
            {
                "name": "iterations",
                "class": "DWord",
                "comments": [
                    "IKの計算回数"
                ]
            },
            {
                "name": "limitation_radian",
                "class": "Float",
                "comments": [
                    "IKの計算1回あたりの制限確度, ラジアン"
                ]
            }
        ]
    },
    "Controller": {
