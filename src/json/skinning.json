{
    "Skinning": {
        "extends": "Section",
        "attributes": [
            {
                "name": "bones",
                "class": "Array",
                "extends": "Bone"
            },
            {
                "name": "influented_num_bones_per_vertex",
                "class": "DWord",
                "comments": [
                    "頂点あたりの最大ウェイト数"
                ]
            },
            {
                "name": "weights",
                "class": "Array",
                "extends": "Weight",
                "comments": [
                    "ウェイトの数はinfluented_num_bones_per_vertex * vertices.size",
                    "頂点ごとのウェイト値を定義"
                ]
            }
        ]
    },
    "Bone": {
        "has_a": ["Digest", "Position"],
        "attributes": [
            {
                "name": "parent_id",
                "references": "Bone"
            },
            {
                "name": "direction",
                "class": "Quaternion",
                "comments": [
                    "グローバルから見た時のボーンの向き",
                    "ボーンのローカル軸に相当"
                ]
            }
        ]
    },
    "Weight": {
        "references": ["Bone"],
        "attributes": [
            {
                "name": "value",
                "class": "Float",
                "comments": [
                    "ウェイト値は w = {w_n| 0 < n < 5} -> w1 + w2 + w3 + w4 = 1"
                ]
            }
        ]
    },
    "Limitation": {
        "references": ["Bone"],
        "attributes": [
            {
                "name": "x",
                "class": "Byte"
            },
            {
                "name": "y",
                "class": "Byte"
            },
            {
                "name": "z",
                "class": "Byte"
            }
        ]
    }
}